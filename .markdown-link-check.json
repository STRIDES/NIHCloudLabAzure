{
  "ignorePatterns": [
    {
      "pattern": "^#"
    },
    {
      "pattern": "^https://platform.openai.com/docs/guides/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/docs",
      "replacement": "https://github.com/STRIDES/NIHCloudLabAzure/tree/main/docs"
    },
    {
      "pattern": "^/notebooks",
      "replacement": "https://github.com/STRIDES/NIHCloudLabAzure/tree/main/notebooks"
    }

  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [403, 200, 206]
}
